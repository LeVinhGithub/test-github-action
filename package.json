{
  "name": "wdio-cucumber-jitera",
  "version": "1.0.0",
  "description": "cucumber testing",
  "main": "index.js",
  "scripts": {
    "test": "wdio run ./wdio.conf.ts",
    "test:tabledef": "yarn clsrp && wdio run ./wdio.conf.ts --suite tabledef",
    "test:apis": "yarn clsrp && wdio run ./wdio.conf.ts --suite tabledef",
    "test:dash": "yarn clsrp && wdio run ./wdio.conf.ts --suite tabledef",
    "report": "allure generate allure-results --clean && allure open ",
    "clsrp": "rm -rf ./allure-results && rm -rf ./allure-report "
  },
  "keywords": [
    "webdriverio",
    "typescript",
    "cucumber",
    "nodejs"
  ],
  "author": "harryle",
  "license": "ISC",
  "dependencies": {
    "@wdio/cli": "^7.13.1",
    "allure-commandline": "^2.13.8"
  },
  "devDependencies": {
    "@wdio/allure-reporter": "^7.13.0",
    "@wdio/cucumber-framework": "^7.13.1",
    "@wdio/local-runner": "^7.13.1",
    "@wdio/spec-reporter": "^7.13.0",
    "chromedriver": "^97.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3",
    "wdio-chromedriver-service": "^7.2.2",
    "webdriverio": "^7.13.1"
  }
}
